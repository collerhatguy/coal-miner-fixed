ion.payload, \n            }\n        default: return state;\n    }\n}\nexport default function useWorker(worker, setMoney, money, multiplier) {\n    const [state, dispatch] = useReducer(reducer, worker);\n    const miningSpeed = 3000;\n\n    const BuyWorker = useCallback(() => {\n        const totalCost = state.cost * multiplier;\n        if (money < totalCost) return;\n        setMoney(prevMoney => prevMoney - totalCost);\n        dispatch({type: ACTIONS.Buy, payload: multiplier})\n    }, [state, multiplier, money])\n\n    const UpgradeWorker = useCallback(() => {\n        if (money < state.productionRateUpgradeCost) return;\n        setMoney(prevMoney => prevMoney - state.productionRateUpgradeCost);\n        dispatch({type: ACTIONS.Upgrade})\n    }, [state, money])\n\n    useEffect(() => {\n        setInterval(() => {\n            console.log(\"mined\")\n            console.log(state.owned)\n            setMoney(prevMoney => prevMoney + (state.owned * state.productionRate))\n        }, miningSpeed)\n    }, [])\n    return [state, BuyWorker, UpgradeWorker];\n}",{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","severity":1,"message":"51","line":12,"column":8,"nodeType":"52","endLine":12,"endColumn":15,"suggestions":"53"},{"ruleId":"50","severity":1,"message":"54","line":35,"column":8,"nodeType":"52","endLine":35,"endColumn":34,"suggestions":"55"},{"ruleId":"50","severity":1,"message":"54","line":41,"column":8,"nodeType":"52","endLine":41,"endColumn":22,"suggestions":"56"},{"ruleId":"50","severity":1,"message":"57","line":49,"column":8,"nodeType":"52","endLine":49,"endColumn":10,"suggestions":"58"},"no-native-reassign",["59"],"no-negated-in-lhs",["60"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'worker.cost'. Either include it or remove the dependency array.","ArrayExpression",["61"],"React Hook useCallback has a missing dependency: 'setMoney'. Either include it or remove the dependency array. If '