uyWorker = useCallback(() => {\n        const totalCost = state.cost * multiplier;\n        if (money < totalCost) return;\n        setMoney(prevMoney => prevMoney - totalCost);\n        dispatch({type: ACTIONS.Buy, payload: multiplier})\n    }, [state, multiplier, money])\n\n    const UpgradeWorker = useCallback(() => {\n        if (money < state.productionRateUpgradeCost) return;\n        setMoney(prevMoney => prevMoney - state.productionRateUpgradeCost);\n        dispatch({type: ACTIONS.Upgrade})\n    }, [state, money])\n\n    useEffect(() => {\n        setInterval(() => {\n            console.log(\"mined\")\n            console.log(state.owned)\n            setMoney(prevMoney => prevMoney + (state.owned * state.productionRate))\n        }, miningSpeed)\n    }, [state])\n    return [state, BuyWorker, UpgradeWorker];\n}",{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":12,"column":8,"nodeType":"53","endLine":12,"endColumn":15,"suggestions":"54"},{"ruleId":"51","severity":1,"message":"55","line":35,"column":8,"nodeType":"53","endLine":35,"endColumn":34,"suggestions":"56"},{"ruleId":"51","severity":1,"message":"55","line":41,"column":8,"nodeType":"53","endLine":41,"endColumn":22,"suggestions":"57"},{"ruleId":"51","severity":1,"message":"58","line":49,"column":8,"nodeType":"53","endLine":49,"endColumn":15,"suggestions":"59"},"no-native-reassign",["60"],"no-negated-in-lhs",["61"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'worker.cost'. Either include it or remove the dependency array.","ArrayExpression",["62"],"React Hook useCallback has a missing dependency: 'setMoney'. Either include it or remove the dependency array. If 'setMoney' changes too often, find the parent component that defines it and wrap that definition in useCallback.",["63"],["64"],"React Hook useEffect has a missing dependency: 'setMoney'. Either include it or remove the dependency array. If 'setM